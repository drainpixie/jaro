{
  "name": "@drainpixie/jaro",
  "version": "1.0.1",
  "type": "module",
  "description": "a small string distance algorithm",
  "license": "LGPL-3.0",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
